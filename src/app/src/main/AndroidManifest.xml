    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.Staff.Home.StaffHomeActivity"></activity>
        <activity android:name=".View.Admin.UserManagementActivity.AddUserActivity" />
        <activity android:name=".View.Staff.UserProfile.StaffUserProfileActivity" />
        <activity android:name=".View.Staff.RequestManagement.RequestCrudActivity.StaffRequestCrudActivity" />
        <activity android:name=".View.Admin.UserManagementActivity.AdminInformationActivity" />
        <activity android:name=".View.Admin.UserRequestActivity.UserRequestActivity" />
        <activity android:name=".View.Staff.RequestManagement.RequestActivity.StaffRequestActivity" />
        <activity android:name=".View.Admin.MainAdminActivity.MainAdminActivity" />
        <activity android:name=".View.Main.LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>